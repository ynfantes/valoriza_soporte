<link rel="stylesheet" href="{{constant('ROOT')}}assets/css/core.min.css">
<link rel="stylesheet" href="{{constant('ROOT')}}assets/css/vendor_bundle.min.css">
<link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Roboto:300,400,500,700&amp;display=swap">
<!-- favicon -->
<link rel="shortcut icon" href="{{constant('ROOT')}}favicon.ico">
<link rel="apple-touch-icon" href="{{constant('ROOT')}}demo.files/logo/icon_512x512.png">
<link rel="manifest" href="{{constant('ROOT')}}assets/images/manifest/manifest.json">
